<% site.posts.forEach(function(post){ %> <% if(post.type === 'homepage'
&&post.section === 'logo design') { %>
<section class="post fourth fifth">
   <h2 class="post-heading"><%= post.title %></h2>
   <div class="post-desc">
      <p><%- post.desc %></p>
   </div>
   <div class="post-content">
      <div class="post-container">
         <i class="fa-solid fa-angle-left"></i>
         <div class="post-media fade">
            <div class="post-media-item" data-slide="1">
               <div class="post-image">
                  <img
                     loading="lazy"
                     src="images/Logo-folio/vntailor.png"
                     alt="image" />
               </div>
            </div>
            <div class="post-media-item" data-slide="2">
               <div class="post-image">
                  <img
                     loading="lazy"
                     src="images/Logo-folio/lotuspa.png"
                     alt="image" />
               </div>
            </div>
            <div class="post-media-item" data-slide="3">
               <div class="post-image">
                  <img
                     loading="lazy"
                     src="images/Logo-folio/tecno.png"
                     alt="image" />
               </div>
            </div>
            <div class="post-media-item" data-slide="4">
               <div class="post-image">
                  <img
                     loading="lazy"
                     src="images/Logo-folio/mentor1.png"
                     alt="image" />
               </div>
            </div>
            <div class="post-media-item" data-slide="5">
               <div class="post-image">
                  <img
                     loading="lazy"
                     src="images/Logo-folio/tmp0.png"
                     alt="image" />
               </div>
            </div>
            <div class="post-media-item" data-slide="6">
               <div class="post-image">
                  <img
                     loading="lazy"
                     src="images/Projects/ttf.png"
                     alt="image" />
               </div>
            </div>
         </div>
         <i class="fa-solid fa-angle-right"></i>
      </div>

      <div class="post-link go-up">
         <a href="/Logofolio" target="_blank">View more</a>
      </div>
   </div>
</section>
<% }}) %>
