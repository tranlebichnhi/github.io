<% site.posts.forEach(function(post){ %> <% if(post.type === 'logofolio' &&
post.id == 3) { %>
<section class="section third reverse">
   <div class="section-wrap">
      <div class="section-content">
         <h1 class=""><%- post.title %></h1>
         <p class=""><%- post.desc %></p>
      </div>
      <div class="section-media">
         <div class="section-image">
            <img src="../images/Logo-folio/lotuspa3.png" alt="img" />
         </div>
         <div class="section-media-container">
            <div class="section-image">
               <img src="../images/Logo-folio/lotuspa2.png" alt="img" />
            </div>
            <div class="section-image">
               <img src="../images/Logo-folio/lotuspa1.png" alt="img" />
            </div>
         </div>
      </div>
   </div>
</section>
<% }}) %>
